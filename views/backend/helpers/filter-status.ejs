<% 
    filterStatusHelper = (filterStatus, keyword, linkPrefix) => {
        let xhtml = '';
        filterStatus.forEach((element, index) => {
            let link = `${linkPrefix}?status=${element.value}`;
            xhtml += `<a href="${link}" class="mr-1 btn btn-sm btn-${element.color}">${element.name} <span class="badge badge-pill badge-light">${element.count}</span></a>`
        });
        return xhtml;
    }
%>
